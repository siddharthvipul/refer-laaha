{#
/**
 * @file
 * Display an iframe with alterable components.
 */
#}
{{ attach_library('vss_custom/youtube_transcript') }}
<iframe{{attributes}} {% if url is not empty %} src="{{ url }}{% if query is not empty %}?{{ query | url_encode }}&enablejsapi=1&modestbranding=1&controls=0{% endif %}{% if fragment is not empty %}#{{ fragment }}{% endif %}" {% endif %}></iframe>
<div id="videoTranscript{{video_id}}" class="videoTranscript" data-language="{{language}}"></div>



