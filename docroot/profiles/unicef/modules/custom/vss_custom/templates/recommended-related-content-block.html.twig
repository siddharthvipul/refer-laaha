
{% for item in content %}

  {% if item.uri %}
    File: uri: {{ file_url(item.uri) }}
  {% endif %}
  {% if item.title %}
    {{ 'Title'|t }}: {{ item.title }}
  {% endif %}
  {% if item.link %}
    {{ 'Link'|t }}:
    {{ item.link }}
  {% endif %}

{% if item.type %}
	{{ 'Type'|t }}:
	{{ item.type }}
{% endif %}

{% endfor %}
